{
  "name": "rickselby/laravel-request-field-types",
  "description": "Allow definitions of common input field types for requests",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Rick Selby",
      "email": "rick@selby-family.co.uk"
    }
  ],
  "require": {
    "laravel/framework": "7.*|8.*|9.*"
  },
  "require-dev": {
    "graham-campbell/testbench": "^5.0",
    "phpunit/phpunit": "^7.0|^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "RickSelby\\LaravelRequestFieldTypes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RickSelby\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RickSelby\\LaravelRequestFieldTypes\\RequestFieldTypesServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": false
  }
}
